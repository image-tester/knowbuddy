#right .rightCorner
{
  content: '';
  width: 50px;
  height: 50px;
  top: 0;
  right: 0;
  position: absolute;
  display: inline-block;
  z-index: -6;
  -webkit-box-shadow: 10px -10px 8px rgba(0, 0, 0, 0.2);
  -webkit-transform: rotate(2deg)
  translate(-14px,20px)
  skew(-20deg);
}
#post-post::before {
  content: '';
  width: 25px;
  height: 20px;
  position: absolute;
  bottom: 0;
  right: 0;
  -moz-border-radius-bottomright: 30px;
  -moz-box-shadow: -2px -2px 5px rgba(0, 0, 0, 0.3);
  -moz-transform: rotate(-13deg)
  translate(-9px,-9px)
  skew(-40deg,-3deg);
  -webkit-border-bottom-right-radius: 30px;
  -webkit-box-shadow: -2px -2px 5px rgba(0, 0, 0, 0.3);
  -webkit-transform: rotate(-20deg)
  skew(-40deg,-3deg)
  translate(-13px,-13px);
}
#post-post::after {
  content: '';
  z-index: -10;
  width: 100px;
  height: 100px;
  position: absolute;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.2);
  display: inline-block;
  -moz-box-shadow: 20px 20px 8px rgba(0, 0, 0, 0.3);
  -moz-transform: rotate(0deg)
  translate(-45px,-20px)
  skew(20deg);
  -webkit-box-shadow: 20px 20px 8px rgba(0, 0, 0, 0.2);
  -webkit-transform: rotate(0deg)
  translate(-45px,-20px)
  skew(20deg);
}
